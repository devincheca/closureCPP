// Devin Checa
using namespace std;
#include <iostream>
#include "closureAPI.h"
int main()
{
	closure seq;
	for (int i = 0; i < 10; i++)
	{
		cout 
			<< seq.fib() 
			<< endl;
	}
	for (int i = 0; i < 10; i++)
	{
		cout << i % 2 == 0 ? seq.adder() : seq.decer();
		cout << endl;
	}
	system("pause");
	return 0;
}
